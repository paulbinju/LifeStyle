        <div class="modal fade" id="modalArticle" role="dialog" style="margin-top:100px;z-index:99999999;font-family:Arial">
            <div class="modal-dialog modal-lg">
                <form method="post" action="@Url.Content("~/Article/Create")"  enctype="multipart/form-data">
                    <div class="modal-content">
                   

                        <div class="modal-body">
                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h3 class="modal-title">Write Article</h3>
                            <br style="clear:both" />

                            <div class="col-lg-12">


                                <b>Category <span style="color:red">*</span></b>

                                <select class="form-control" name="CategoryId" required style="font-size:18px;height:40px;">
                                    <option value="">Select</option>
                                    @if (ViewBag.AdminUser == "NO")
                                    {
                                        foreach (var cat in ViewBag.Category)
                                        {
                                            <option value="@cat.CategoryId">@cat.CategoryName</option>
                                        }
                                    }
                                </select>


                            </div>

                            <br style="clear:both" />
                            <br style="clear:both" />
                            <div class="col-lg-12">


                                <b>Title <span style="color:red">*</span></b>

                                <textarea name="Title" class="form-control" required rows="2" style="font-size:18px;"></textarea>


                            </div>

                            <br style="clear:both" />
                            <br style="clear:both" />
                            <div class="col-lg-12">

                                <b>Cover Image  <span style="color:red">*</span></b>

                                <input type="file" name="file" id="file" class="form-control" required style="font-size:18px;height:40px;" />


                            </div>
                            <script>
                                $('#file').bind('change', function () {

                                    var extension = $('#file').val().split('.').pop().toLowerCase();
                                    if ($.inArray(extension, ['gif', 'png', 'jpg', 'jpeg']) == -1) {
                                        alert('Only .jpg, .jpeg, .png, .gif are allowed');
                                        $('#file').val("");
                                        return false;
                                    }

                                });
                            </script>

                            <br style="clear:both" />
                            <br style="clear:both" />

                            <button type="submit" class="btn btn-danger pull-right" style="background-color:#de1a22; color:#ffffff;margin:0 12px 0 0">NEXT</button>
                            <br style="clear:both" />


                        </div>

                       
                    </div>
                </form>
